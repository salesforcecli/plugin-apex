[
  {
    "command": "apex:execute",
    "plugin": "@salesforce/plugin-apex",
    "flags": ["apex-code-file", "api-version", "json", "target-org"],
    "alias": ["force:apex:execute"]
  },
  {
    "command": "apex:log:get",
    "plugin": "@salesforce/plugin-apex",
    "flags": ["api-version", "json", "log-id", "number", "output-dir", "target-org"],
    "alias": ["force:apex:log:get"]
  },
  {
    "command": "apex:log:list",
    "plugin": "@salesforce/plugin-apex",
    "flags": ["api-version", "json", "target-org"],
    "alias": ["force:apex:log:list"]
  },
  {
    "command": "apex:log:tail",
    "plugin": "@salesforce/plugin-apex",
    "flags": ["api-version", "color", "debug-level", "json", "skip-trace-flag", "target-org"],
    "alias": ["force:apex:log:tail"]
  },
  {
    "command": "apex:test:report",
    "plugin": "@salesforce/plugin-apex",
    "flags": ["api-version", "code-coverage", "json", "output-dir", "result-format", "target-org", "test-run-id"],
    "alias": ["force:apex:test:report"]
  },
  {
    "command": "apex:test:run",
    "plugin": "@salesforce/plugin-apex",
    "flags": [
      "api-version",
      "class-names",
      "code-coverage",
      "detailed-coverage",
      "json",
      "output-dir",
      "result-format",
      "suite-names",
      "synchronous",
      "target-org",
      "test-level",
      "tests",
      "wait"
    ],
    "alias": ["force:apex:test:run"]
  }
]
